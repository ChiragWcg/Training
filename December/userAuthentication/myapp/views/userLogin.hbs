            <div class="mb-3">
                <label for="email" class="form-label">Enter email address or contact number</label>
                <input type="text" class="text form-control" id="email" name="email">
            </div><br>

            <div class="mb-3">
                <label for="password" class="form-label"> Enter your password</label>
                <input type="password" class="password form-control" id="password" name="password">
            </div><br>

            <button type="submit" id="btn" name="btn" class="btn btn-primary">Login</button>
            <div class="btns"></div>

        </form>
